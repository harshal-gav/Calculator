{
    "hosting": {
        "public": "out",
        "cleanUrls": true,
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "redirects": [
            {
                "source": "**",
                "destination": "https://calculator-all.com/:path",
                "type": 301
            }
        ]
    }
}